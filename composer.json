{
  "name": "yuncms/yuncms-app-basic",
  "description": "YUNCMS Basic Project Template",
  "keywords": [
    "yuncms",
    "yii2",
    "framework",
    "basic",
    "project template"
  ],
  "homepage": "http://www.yuncms.net/",
  "type": "project",
  "license": "MIT",
  "support": {
    "issues": "https://github.com/yuncms/yuncms-app-basic/issues?state=open",
    "source": "https://github.com/yuncms/yuncms-app-basic"
  },
  "minimum-stability": "dev",
  "require": {
    "php": ">=7.1.0",
    "yuncms/framework": "1.0.x-dev",
    "yiisoft/yii2-bootstrap": "~2.0.0",
    "vlucas/phpdotenv": "^2.4.0",
    "yuncms/broadcast-aliyun": "~1.0",
    "yuncms/broadcast-qcloud": "~1.0"
  },
  "require-dev": {
    "yiisoft/yii2-debug": "~2.0.0",
    "league/flysystem-aws-s3-v3": "~1.0",
    "league/flysystem-azure": "~1.0",
    "league/flysystem-cached-adapter": "~1.0",
    "league/flysystem-copy": "~1.0",
    "league/flysystem-dropbox": "~1.0",
    "league/flysystem-gridfs": "~1.0",
    "league/flysystem-rackspace": "~1.0",
    "league/flysystem-replicate-adapter": "~1.0",
    "league/flysystem-sftp": "~1.0",
    "league/flysystem-webdav": "~1.0",
    "league/flysystem-ziparchive": "~1.0",
    "xxtime/flysystem-aliyun-oss": "~1.2",
    "overtrue/flysystem-qiniu": "~1.0",
    "jellybool/flysystem-upyun": "~1.0",
    "freyo/flysystem-qcloud-cos-v4": "~2.0",
    "freyo/flysystem-qcloud-cos-v5": "~1.0"
  },
  "config": {
    "process-timeout": 1800
  },
  "scripts": {
    "post-root-package-install": [
      "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
    ]
  },
  "repositories": [

    {
      "type": "composer",
      "url": "https://asset-packagist.org"
    }
  ]
}
